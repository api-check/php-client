{
  "name": "api-check/php-client",
  "description": "A wrapper around ApiCheck get to validate addresses.",
  "keywords": [
      "apicheck",
      "laravel",
      "addressvalidation",
      "postalcodeapi",
      "addressapi"
  ],
  "homepage": "https://apicheck.nl/",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "ApiCheck",
      "email": "support@apicheck.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.4|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "3.8.*|4.*|5.*|6.*",
    "phpunit/phpunit": "^7.0|^8.0|^9.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ApiCheck\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiCheck\\Api\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  }
}